{
  "name": "@xirelogy/vue-minimal",
  "version": "3.5.1",
  "description": "Minimal types for vue's cross project Typescript compatibility",
  "repository": {
    "type": "git",
    "url": "https://github.com/xirelogy/vue-minimal"
  },
  "type": "module",
  "main": "dist/vue-minimal.umd.cjs",
  "module": "dist/vue-minimal.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/vue-minimal.umd.cjs",
      "import": "./dist/vue-minimal.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build && vue-tsc",
    "preview": "vite preview"
  },
  "peerDependencies": {
    "vue": "3.5"
  },
  "devDependencies": {
    "@types/node": "^18.17.5",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-typescript": "^14.0.0",
    "typescript": "5.5",
    "vite": "^4.4.5",
    "vue-tsc": "^2.0"
  }
}
